{
  "name": "wallets-generator",
  "version": "1.1.0",
  "description": "Generate crypto wallets (bitcoin, ethereum...)",
  "repository": {
    "type": "git",
    "url": "git@github.com:rhrn/wallets-generator.git"
  },
  "main": "index.js",
  "bin": {
    "wallets-generator": "dist/cli.js"
  },
  "scripts": {
    "build": "babel src -d dist",
    "dev": "babel src -d dist --watch",
    "lint": "standard 'src/**/*.js'",
    "jest": "jest src/",
    "test": "npm run lint && npm run jest"
  },
  "standard": {
    "globals": [
      "beforeAll",
      "afterAll",
      "it",
      "describe",
      "expect"
    ]
  },
  "keywords": [
    "crypto",
    "balance",
    "bitcoin",
    "btc",
    "litecoin",
    "ltc",
    "gold",
    "btg",
    "cache",
    "bch",
    "ethereum",
    "eth",
    "classic",
    "etc",
    "wallet"
  ],
  "author": "@rhrn",
  "license": "MIT",
  "dependencies": {
    "bgoldjs-lib": "^3.3.2",
    "bitcoincashjs": "^0.1.10",
    "bitcoinjs-lib": "^3.3.1",
    "concurrency-promise": "^1.0.1",
    "ethereumjs-wallet": "^0.6.0",
    "etherscan-api": "^8.1.0",
    "fs-extra": "^4.0.2",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^21.2.1",
    "standard": "^10.0.3"
  }
}
